{
  "include": [
    "."
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__"
  ],
  "pythonVersion": "3.11",
  "pythonPlatform": "Darwin",
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "pythonPlatform": "Darwin",
      "extraPaths": [
        "/opt/miniconda3/lib/python3.11/site-packages",
        "/opt/miniconda3/envs/chatbot-env/lib/python3.11/site-packages"
      ]
    }
  ],
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": false,
  "pythonPath": "/opt/miniconda3/envs/chatbot-env/bin/python"
}
